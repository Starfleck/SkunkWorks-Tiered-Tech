<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <VoxelMaterials> 
  
  	    <!-- Iridium -->
  
    <VoxelMaterial xsi:type="MyObjectBuilder_Dx11VoxelMaterialDefinition">
      <Id>
        <TypeId>VoxelMaterialDefinition</TypeId>
        <SubtypeId>iridium</SubtypeId>
      </Id>
      <MaterialTypeName>Rock</MaterialTypeName>
      <MinedOre>Iridium</MinedOre>
      <MinedOreRatio>0.3</MinedOreRatio>
      <CanBeHarvested>false</CanBeHarvested>
      <IsRare>true</IsRare>
      <SpawnsInAsteroids>false</SpawnsInAsteroids>
      <AsteroidGeneratorSpawnProbabilityMultiplier>5</AsteroidGeneratorSpawnProbabilityMultiplier>

      <MinVersion>1</MinVersion>
	  
      <VoxelHandPreview>Textures\Voxels\AlienIce_Thumbnail.dds</VoxelHandPreview>
      <ColorMetalXZnY>Textures\Voxels\Iridium_marble_cm.dds</ColorMetalXZnY>
      <NormalGlossY>Textures\Voxels\MoonSoil_01_ForAxisXZ_ng.dds</NormalGlossY>
      <ExtXZnY>Textures\Voxels\iridium_marble_add.dds</ExtXZnY>

      <InitialScale>6</InitialScale>
      <ScaleMultiplier>20</ScaleMultiplier>
      <InitialDistance>9</InitialDistance>
      <DistanceMultiplier>40</DistanceMultiplier>
      <TilingScale>60</TilingScale>

      <ExtDetailScale>0.005</ExtDetailScale>
	  
      <Far1Distance>100</Far1Distance>
      <Far1Scale>90</Far1Scale>	  
	  
      <Far2Distance>250</Far2Distance>
      <Far2Scale>120</Far2Scale>
	  
	  <Far3Distance>2000</Far3Distance>
      <Far3Color>
        <X>0.17</X>
        <Y>0.19</Y>
        <Z>0.20</Z>
        <W>1.0</W>
      </Far3Color>
	  
      <Friction>0.15</Friction>
      <Restitution>0.9</Restitution>
      <ColorKey Hex="#DD9B28"/>
	  
      <LandingEffect>Landing_Jet_Ground_Dust</LandingEffect>
    </VoxelMaterial>
	
	    <!-- Uraninite_01 -->
